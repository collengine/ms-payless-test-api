{
   "executionRoleArn": "arn:aws:iam::812830320641:role/ecsTaskExecutionRole",
   "taskRoleArn": "arn:aws:iam::812830320641:role/ecsTaskExecutionRole",
   "containerDefinitions": [
      {
         "name": "proto-solution-prof-be",
         "image": "<IMAGE_NAME>",
         "cpu": 2048,
         "memory": 8192,
         "essential": true,
         "portMappings": [
            {
               "containerPort": 3000,
               "hostPort": 3000
            }
         ],
         "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
               "awslogs-group": "/services/proto-solution-prof-be",
               "awslogs-region": "eu-west-1",
               "awslogs-stream-prefix": "solution-prof-be"
            }
         },
         "environment": [
            {
               "name": "ENV",
               "value": "proto-prof"
            },
            {
               "name": "PDF_TASK_DEFINITION_FAMILY",
               "valueFrom": "arn:aws:ssm:us-west-2:171301263985:parameter/proto/solution-prof-be/PDF_TASK_DEFINITION_FAMILY"
            },
            {
               "name": "PDF_CLUSTER",
               "valueFrom": "arn:aws:ssm:us-west-2:171301263985:parameter/proto/solution-prof-be/PDF_CLUSTER"
            },
            {
               "name": "PDF_SUBNET_IDS",
               "valueFrom": "arn:aws:ssm:us-west-2:171301263985:parameter/proto/solution-prof-be/PDF_SUBNET_IDS"
            }
         ]
      }
   ],
   "requiresCompatibilities": ["FARGATE"],
   "networkMode": "awsvpc",
   "cpu": "2048",
   "memory": "8192",
   "family": "proto-solution-prof-be"
}
